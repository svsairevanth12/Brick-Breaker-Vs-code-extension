{
  "name": "brick-breaker-game",
  "displayName": "Brick Breaker Game",
  "description": "A classic brick breaker game playable within VS Code",
  "version": "1.0.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "game",
    "brick breaker",
    "breakout",
    "entertainment"
  ],

  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "brickBreaker.openGame",
        "title": "Open Brick Breaker Game",
        "category": "Brick Breaker",
        "icon": "$(game)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "brickBreaker.openGame",
          "title": "Open Brick Breaker Game"
        }
      ]
    },
    "views": {
      "brickBreakerView": [
        {
          "id": "brickBreakerGameView",
          "name": "Brick Breaker Game",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "brickBreakerView",
          "title": "Brick Breaker Game",
          "icon": "$(game)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "npx @vscode/vsce package",
    "publish": "npx @vscode/vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svsairevanth12/Brick-Breaker-Vs-code-extension.git"
  },
  "bugs": {
    "url": "https://github.com/svsairevanth12/Brick-Breaker-Vs-code-extension/issues"
  },
  "homepage": "https://github.com/svsairevanth12/Brick-Breaker-Vs-code-extension#readme",
  "license": "MIT",
  "icon": "icon.png"
}
